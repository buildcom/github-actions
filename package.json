{
  "name": "things-must-be-labeled",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub bot to automatically label things",
  "author": "Lisa Dean <thingsmustbelabeled@lisadean.net>",
  "license": "ISC",
  "homepage": "https://github.com/lisadean/things-must-be-labeled",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "test": "jest",
    "dev": "nodemon src/main.ts",
    "dev:debug": "nodemon --exec 'node --inspect --require ts-node/register src/main.ts'"
  },
  "dependencies": {
    "probot": "^11.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.19",
    "jest": "^26.6.3",
    "nock": "^13.0.5",
    "nodemon": "^2.0.12",
    "smee-client": "^1.2.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "prettier": {
	"printWidth": 140,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "none",
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"arrowParens": "always",
	"endOfLine": "lf"
}
}
